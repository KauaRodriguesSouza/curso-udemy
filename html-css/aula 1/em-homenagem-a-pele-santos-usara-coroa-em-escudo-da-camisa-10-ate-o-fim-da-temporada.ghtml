const _getLocalStorage = (key) => {
    return JSON.parse(localStorage.getItem(key));
}

const _setLocalStorage = (labelStorage, data) => {
    localStorage.setItem(labelStorage, JSON.stringify(data));
}
// END BASE FUNCTIONS

// Função para verificar se o LGPD já foi aceito.
const acceptedLGPD = () => {
    let lgpdCookieName = 'cookie-banner-consent-accepted';
    let lgpdCookie = document.cookie.split(';').filter(s => s.includes(lgpdCookieName));
    let lgpdCookieValue = lgpdCookie.some(s => s.includes('true'));
    return lgpdCookieValue;
}

const rgb2hex = (envColor) => {
    let red = envColor.split(',')[0].replace(/[^0-9\.]+/g, '');
    let green = envColor.split(",")[1].replace(/[^0-9\.]+/g, "");
    let blue = envColor.split(",")[2].replace(/[^0-9\.]+/g, "");
    let rgb = blue | (green << 8) | (red << 16);
    return '#' + (0x1000000 + rgb).toString(16).slice(1)
}

const gettingUserData = () => {
    let userId = utag_data.glbdt_globoid || "";
    let teamHex = rgb2hex(envColor);
    let teamurl = document.location.href || "";
    let teamName = equipe;
    let team = strTeam;

    return {
        userId: userId,
        teamHex: teamHex,
        teamurl: teamurl,
        teamName: teamName,
        team: team
    };
}

const returnPageType = () => {
    let teamPage = window.location.pathname.indexOf("/futebol/times/") != -1;
    let multiContentPages = window.location.pathname.indexOf("/noticia/") != -1 ||
                            window.location.pathname.indexOf("/post/") != -1;
    let sandbox = window.location.pathname.indexOf("/sandbox/desktop/") != -1;
    let pageType = "";

    if ((teamPage && multiContentPages)) {
        pageType = "multicontent";
    } else if (teamPage || sandbox) {
        pageType = "teamPage";
    } else {
        pageType = false;
    }

    return pageType;
}

const denyingTeam = () => {
    let sportsTeam = _getLocalStorage("sportsTeam");

    for (let index = 0; index < sportsTeam.length; index++) {
        if (sportsTeam[index][equipe] != undefined) {
            sportsTeam[index][equipe]["isMyTeam"] = false;
        }
    }
    _setLocalStorage("sportsTeam", sportsTeam);
}

const creatingSportsTeamLS = () => {
    let sportsTeam = _getLocalStorage("sportsTeam") || [];
    let userData = gettingUserData();
    let dataTeam = {};

    dataTeam[equipe] = {
        id: teamId,
        name: equipe,
        pageviews: 1,
        globoid: userData.userId,
        hex: userData.teamHex,
        url: userData.teamurl
    };
    sportsTeam.push(dataTeam)

    _setLocalStorage("sportsTeam", sportsTeam);
}

const validatingSportsTeamLS = () => {
    let sportsTeam = window.localStorage.getItem("sportsTeam")

    if (sportsTeam == null) {
        creatingSportsTeamLS();
    } else {
        if (sportsTeam.indexOf(equipe) === -1) {
            creatingSportsTeamLS();
        } else {
            sportsTeam = JSON.parse(sportsTeam);
            for (let index = 0; index < sportsTeam.length; index++) {
                if (sportsTeam[index][equipe]) {
                    updatingSportsTeamLS();
                }
            }
        }
    }
}

const updatingSportsTeamLS = () => {
    let sportsTeam = _getLocalStorage("sportsTeam");

    for (let index = 0; index < sportsTeam.length; index++) {
        if (sportsTeam[index][equipe]) {
            sportsTeam[index][equipe].pageviews++;
        }
    }
    _setLocalStorage("sportsTeam", sportsTeam);
}

const defineCustomBadge = () => {
    let badgeTeam = document.createElement("img");
    let escudoSrc = "";

    // Creating Badge Team
    if (equipe !== "") {
        let ctMosaico = document.querySelector(".mosaico");
        let mosaicos = ctMosaico.querySelectorAll(".mosaico__escudo--img");
        let divMosaicos = ctMosaico.querySelectorAll(".mosaico__escudo");

        for (let i = 0; i < mosaicos.length; i++) {
            if (mosaicos[i].dataset.src.toLowerCase().includes(equipe.toLowerCase())) {
                escudoSrc = mosaicos[i].dataset.src;
                teamId = divMosaicos[i].dataset.idEquipeSde;
                break;
            }
        }

        badgeTeam.setAttribute("src", escudoSrc);
        badgeTeam.setAttribute("alt", strTeam);
        badgeTeam.classList.add("badge-user-team");
    }

    // Displaying Badge Team
    if (
        badgeTeam.getAttribute("src") !== null ||
        badgeTeam.getAttribute("src") !== ""
    ) {
        document.querySelector(".ct-escolha-time .badge").prepend(badgeTeam)
        document.querySelector(".ct-escolha-time .badge img").dataset.idEquipeSde = teamId;
    }
}

const _sendEventToHorizon = (answer) => {
    console.debug(prefixName, "Horizon", answer);
    let optionsClick;

    switch (answer) {
        case "get_geql":
            optionsClick = {
                id: "common-event",
                version: "0.1",
                contentType: "common",
                properties: {
                    eventCategory: "componente-escolha-time",
                    eventAction: "get",
                    eventLabel: "geql"
                }
            }

            break;
        
        case "error_geql":
            optionsClick = {
                id: "common-event",
                version: "0.1",
                contentType: "common",
                properties: {
                    eventCategory: "componente-escolha-time",
                    eventAction: "error",
                    eventLabel: "geql"
                }
            }

            break;

        case "impressions":
            optionsClick = {
                id: "common-event",
                version: "0.1",
                contentType: "common",
                properties: {
                    eventCategory: "componente-escolha-time",
                    eventAction: "impression",
                    eventLabel: equipe
                }
            }

            break;

        case "sim":
        case "nao":
            optionsClick = {
                id: "common-event",
                version: "0.1",
                contentType: "common",
                properties: {
                    eventCategory: "componente-escolha-time",
                    eventAction: "feedback",
                    eventLabel: answer
                }
            }
            break;

        case "teamchoose":
            optionsClick = {
                id: "profile-ge-team-choose",
                version: "1.0",
                contentType: "home",
                properties: {
                    team: equipe
                }
            }
            break;
    }

    if (horizonClient) {
        try {
            horizonClient.send(optionsClick);
        } catch (error) {
            console.error(prefixName, "Erro ao enviar dados para o horizon:", error);
        }
    }
}

const enableClickEvents = (teamLinks) => {
    let userData = gettingUserData();
    let dataTeam;

    // Botao SIM
    teamLinks[0].addEventListener("click", function (event) {
        event.preventDefault();
        dataTeam = {
            userTeam: {
                id: teamId,
                name: equipe,
                team: strTeam,
                userId: userData.userId,
                url: userData.teamurl,
                hex: userData.teamHex
            }
        };

        console.debug(prefixName, "SIM", strTeam, teamId, userData.teamurl);
        window.ge.user.setTeam(strTeam, teamId, userData.teamurl);

        _sendEventToHorizon('sim');
        _sendEventToHorizon('teamchoose');

        showFeedback();
    });

    // Botao NAO
    teamLinks[1].addEventListener("click", function (event) {
        event.preventDefault();
        console.debug(prefixName, "NÃO");

        _sendEventToHorizon('nao');

        denyingTeam()
        hideTicker(500);
    });
}

const colorizeTicker = () => {
    let teamLinks = document.querySelectorAll(".ct-escolha-time .time-btn");

    teamLinks[0].style.color = "#FFFFFF";
    teamLinks[0].style.background = envColor;
    teamLinks[1].style.color = envColor;
    ctEscolhaTimeBlock.style.background = envColor;

    defineCustomBadge();
    enableClickEvents(teamLinks);
}

const hideTicker = (hideTime) => {
    let hTime = hideTime || 6000;
    let animationType = "animationSlideUp"; //"animationRevealBlock"
    setTimeout(function () {
        if (animationType == "animationRevealBlock") {
            detalhesCtEscolhaTime.classList.add("fadeOutText")
            ctEscolhaTime.classList.add("fadeOut")
            ctEscolhaTimeBlock.classList.add("closeBlock")
        }
        if (animationType == "animationSlideUp") {
            ctEscolhaTime.classList.add("slideOutDown");
        }
    }, hTime);
}

const showTicker = () => {
    let bottomBar = document.querySelector(".bottom-bar-navigation--ready");
    let animationType = "animationSlideUp"; //"animationRevealBlock"

    if (bottomBar === null && window.cdaaas.SETTINGS.MOBILE_GROUP === "smart") {
        ctEscolhaTime.style.bottom = 0;
    }

    if (animationType == "animationRevealBlock") {
        detalhesCtEscolhaTime.classList.add("fadeInText");
        ctEscolhaTime.classList.add("fadeIn");
        ctEscolhaTimeBlock.classList.add("revealBlock");
    }
    if (animationType == "animationSlideUp") {
        ctEscolhaTime.classList.add("slideInUp");
    }

    ctEscolhaTime.style.borderColor = envColor;
    ctEscolhaTime.removeAttribute("style");

    _sendEventToHorizon('impressions');
    colorizeTicker();
}

const showFeedback = () => {
    const ctFeedback = document.querySelector(".ct-escolha-time .ct-feedback");
    ctFeedback.style.height = "75px";
    ctFeedback.style.opacity = 1;

    hideTicker();
}

const configTickerWithTeam = () => {
    let currentTeam = document.querySelector(".header-editoria--link").text || equipe;
    document.querySelector(
        ".ct-escolha-time .titulo strong"
    ).innerHTML = currentTeam;

    showTicker();
}

const validateUserHasDataGEQL = () => {
    window.userData = false;
    window.userTeamGeqlAttr = undefined;
    let userTeam = localStorage.getItem("userTeam");
    
    if( userTeam === null ) { // Only if dont have data in LS
        ge.user.getTeam().then(data => {
            window.userTeamGeqlAttr = data;
            console.debug(prefixName, 'userTeamGeqlAttr:', window.userTeamGeqlAttr);
        })

        try {
            ge.user.getUserData().then(data => {
                window.userData = data;
                _sendEventToHorizon("get_geql");
                console.debug(prefixName, 'validade if user has data on GEQL:', window.userData);
            }).catch(() => {
                console.debug(prefixName, 'init Component');
                _init();
            });
        } catch(error) {
            _sendEventToHorizon("error_geql");
            console.error(prefixName, 'get data from GEQL:', error);
        }
    }
}

function validateTeamAttributes() {
    let userTeamAttr = localStorage.getItem("userTeam");
    let sportsTeamAttr = _getLocalStorage("sportsTeam");
    let status = true;
    
    if ( window.whichPage === "multicontent" ) { // Inferencia Modelo usuários
        if (window.userData !== false && window.userData.futebol.name !== equipe) {
            console.debug(prefixName, 'Tem Time via GEQL Cartola, mas Time difere da equipe do MC');
            status = false;

            return false;
        }
    }
    
    if( userTeamAttr === null ) { // nao tem time LS
        console.debug(prefixName, 'Não tem userTeam no LocalStorage');

        if( sportsTeamAttr !== null ) {
            for (let index = 0; index < sportsTeamAttr.length; index++) {
                let myTeam = sportsTeamAttr[index][equipe];
        
                if ( myTeam !== undefined && myTeam["isMyTeam"] !== undefined &&  myTeam["isMyTeam"] === false) {
                    console.debug(prefixName, 'Equipe não é o Time do Coração.');
                    status = false;

                    return false;
                }
            }
        }

        console.debug(prefixName, 'SportsTeam do Time Atual não definido no LS.');

        if ( window.userTeamGeqlAttr === undefined ) { // nao tem time GEQL
            console.debug(prefixName, 'Não tem time do GEQL');
        } else {
            console.debug(prefixName, 'Tem time via GEQL');
            status = false;
        }
    } else {
        console.debug(prefixName, 'Time do Coração já definido.');
        status = false;
    }
    
    return status;
}

const _init = () => {
    // Init user attributes validation
    let checkSandbox = window.location.pathname.indexOf("/sandbox/desktop/") != -1;
    let validateAttr = validateTeamAttributes();

    if ( validateAttr ) {
        // Só continua com o popin de 'escolha de time' se LGPD já foi aceito.
        // Evita que um popin fique por cima do outro.
        // Por questões juridicas o popin LGPD tem prioridade.
        if (!acceptedLGPD() && !checkSandbox) {
            console.error(prefixName, 'Não continuar, LGPD ainda não aceito.');
            return
        } else {
            console.debug(prefixName, 'Continuar, LGPD já aceito.');
        }

        configTickerWithTeam();
        validatingSportsTeamLS()
    }
}

document.addEventListener("readystatechange", function (event) {
    if (document.readyState === 'complete') {
        window.whichPage = returnPageType();
        window.cq = window.cq || []
        window.cq.push({
            ready: (cadun) => {
                cadun.Event.subscribe("userInfoAvailable", () => {
                    userInfo = window.Cadun.user.me
                })
            }
        })

        // Only for Pagina de Times and (Logado && Multicontent)
        if (window.whichPage === "teamPage" || (userInfo.status === "authorized" && window.whichPage === "multicontent")) {
            validateUserHasDataGEQL();
        }
    }
});

